{
    "name": "itseasy/php-queue",
    "description": "PHP Queue Manager",
    "type": "library",
    "keywords": [
        "amqlib"
    ],
    "config": {
        "platform": {
            "php": "7.4.30",
            "ext-sockets": "1"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "php-amqplib/php-amqplib": "^3.0",
        "symfony/console": "^5.3",
        "laminas/laminas-stdlib": "^3.4",
        "laminas/laminas-log": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Itseasy\\Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itseasy\\Queue\\Test\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wiryonolau",
            "email": "wiryonolau@gmail.com"
        }
    ],
    "require-dev": {
        "php-di/php-di": "^6.3",
        "phpunit/phpunit": "^9.5",
        "laminas/laminas-config-aggregator": "^1.4"
    }
}
